{
  "name": "ed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "@material-ui/icons": "^1.1.0",
    "bfj": "5.2.0",
    "browserfs": "^1.4.3",
    "d3": "^5.5.0",
    "d3-bboxCollide": "^1.0.4",
    "d3-flextree": "^2.1.1",
    "hammerjs": "^2.0.8",
    "isomorphic-git": "^0.35.1",
    "jss-camel-case": "^6.1.0",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.2",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-jss": "^8.6.0",
    "resolve": "1.6.0",
    "util.promisify": "^1.0.0",
    "uuid": "^3.3.2",
    "whatwg-fetch": "2.0.4",
    "wolfy87-eventemitter": "^5.2.5"
  },
  "scripts": {},
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/react-dom": "^16.0.7",
    "@types/util.promisify": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "8.5.0",
    "babel-loader": "^8.0.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "css-loader": "0.28.11",
    "html-webpack-plugin": "3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "2.1.5",
    "style-loader": "0.21.0",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
